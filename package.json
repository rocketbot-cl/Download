    "linux": true,
    "windows":true,
    "mac":true,
    "docker":true,
    "name": "Download",
    "dependencies": {
        "requests": "2.21.0"
    },
    "title": {
        "en": "Download",
        "es": "Download",
        "cn": "Download"
    },
    "icon":"data:image/svg+xml;base64,",
    "children": [

        {
            "en": {
                "title": "Download File",
                "description": "Download a file, can rename",
                "title_options": null,
                "options":null
            },
            "es": {
                "title": "Descargar un Archivo",
                "description": "Puede descargar un archivo y renombrarlo",
                "title_options": null,
                "options":null
            },
            "form": {
                "css": "modal-lg",
                "inputs": [
                    {
                        "type": "input",
                        "placeholder": {
                            "es": "https://abc.com/a.txt",
                            "en": "https://abc.com/a.txt"
                        },
                        "title": {
                            "es": "Url del archivo:",
                            "en": "Url downlaod:"
                        },
                        "help": {
                            "es": "URL del archivo de descarga",
                            "en": "Url to download file"
                        },
                        "id": "url",
                        "css": "col-md-12"
                    },
                    {
                        "type": "folder_select",
                        "placeholder": {
                            "es": "/home/user/download",
                            "en": "/home/user/download"
                        },
                        "title": {
                            "es": "Carpeta de descarga:",
                            "en": "Folder to download:"
                        },
                        "help": {
                            "es": "Seleccione una carpeta donde descargar",
                            "en": "Select folder to download"
                        },
                        "id": "file_folder",
                        "css": "col-md-12"
                    },
                    {
                        "type": "input",
                        "placeholder": {
                            "es": "ABC.txt",
                            "en": "ABC.txt"
                        },
                        "title": {
                            "es": "Nombre de Archivo:",
                            "en": "File Name:"
                        },
                        "help": {
                            "en": "Empty to no rename the downloaded file",
                            "es": "Vacío para no cambiar el nombre del archivo"
                        },
                        "id": "file_name",
                        "css": "col-md-6"
                    },
                    {
                        "type": "input",
                        "placeholder": {
                            "es": "{resultado}",
                            "en": "{result}"
                        },
                        "title": {
                            "es": "Resutlado",
                            "en": "Result"
                        },
                        "help": {
                            "en": "Variable where the result is stored without {}",
                            "es": "Variable donde se almacena el resultado sin {}"
                        },
                        "id": "result",
                        "css": "col-md-6"
                    }

                ]
            },
            "video_youtube": "",
            "icon":"data:image/svg+xml;base64,",
            "module": "Download",
            "module_name": "Download",
            "visible": true,
            "options": false,
            "father": "module",
            "group": "scripts",
            "linux": true,
            "windows":true,
            "mac":true,
            "docker":true
        }
    ]
}
